{
  "name": "Event",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true,
      "defaultFn": "uuidv4"
    },
    "type": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "object"
    },
    "created": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
